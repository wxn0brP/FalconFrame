{
    "name": "@wxn0brp/falcon-frame",
    "version": "0.6.2",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "author": "wxn0brP",
    "license": "MIT",
    "type": "module",
    "description": "Lightweight modular TypeScript web framework",
    "homepage": "https://github.com/wxn0brP/FalconFrame",
    "repository": {
        "type": "git",
        "url": "https://github.com/wxn0brP/FalconFrame.git"
    },
    "keywords": [
        "http",
        "web",
        "framework",
        "typescript",
        "modular",
        "lightweight",
        "middleware",
        "middleware-first",
        "middleware-chain"
    ],
    "scripts": {
        "build": "tsc && tsc-alias"
    },
    "devDependencies": {
        "@types/node": "*",
        "tsc-alias": "*",
        "typescript": "*"
    },
    "dependencies": {
        "@wxn0brp/lucerna-log": "^0.2.0"
    },
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js",
            "import": "./dist/index.js"
        },
        "./*": {
            "types": "./dist/*.d.ts",
            "default": "./dist/*.js",
            "import": "./dist/*.js"
        }
    }
}
